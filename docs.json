{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Weights",

  "colors": {
    "primary": "#ffffff",
    "light": "#07C983",
    "dark": "#444444"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "Examples",
            "pages": [
              "examples/image-generation-journey",
              "examples/video-generation-journey",
              "examples/lora-training-journey",
              "examples/rvc-voice-journey",
              "examples/song-generation-journey"
            ]
          },
          {
            "group": "API reference",
            "openapi": "https://app.stainless.com/api/spec/documented/weights/openapi.documented.yml"
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.weights.com",
          "icon": "book-open-cover"
        },
        {
          "anchor": "API Keys",
          "href": "https://www.weights.com/weights-api",
          "icon": "key"
        }
      ]
    }
  },
  "logo": {
    "dark": "/logo/logo.svg",

    "light": "/logo/logo.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@weights.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://www.weights.com/weights-api"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/weights-ai"
    }
  }
}
